<!-- sb code for appear delivery message on home page--->
{%- assign templateBase = template -%}{%- if templateBase == 'index' -%}{% assign collection = collections %}{% assign tagCount = 0 %}{% assign collectionCount = 0 %}<div class='sb_info metafield_update_home_page' style='display:none;' ><div class='sb_store_date'>{{ "now" | date: "%b %d, %Y %H:%M" }}</div><div >Product available : <span class='product_available'>{{product.available}}</span></div><div >Product Id : <span class='product_id'>{{product.id}}</span></div><div >Product Handle : <span class='product_handle'>{{product.handle}}</span></div><div >Product Vendor : <span class='product_vendor'>{{product.vendor}}</span></div><div >Product Type : <span class='product_type'>{{product.type}}</span></div><div >Product Tag : {% for getproduct_tag in product.tags %} <div> {{tagCount}} : <span class='product_tag product_tag_{{tagCount}}'>{{getproduct_tag}}</span>  {% assign tagCount = tagCount | plus: 1 %}  </div>{% endfor %}</div></div>{% endif %}
