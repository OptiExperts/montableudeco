{%- comment -%} <!---------------- LIQUID RULES ----------------> {%- endcomment -%}



{% if section.settings.slider_mobile or section.blocks.size > 4 %}{% assign slider = true %}{% else %}{% assign slider = false %}{% endif %}
{% if section.blocks.size >= 4 %}{% assign blocks_num = 4 %}{% else %}{% assign blocks_num = section.blocks.size %}{% endif %}



{%- comment -%} <!---------------- STYLES ----------------> {%- endcomment -%}



<style> 
  /* svg badges rules */
  #shopify-section-{{ section.id }} .white{fill:#fff} 
  #shopify-section-{{ section.id }} .badges_secondary_color{fill:{{section.settings.badges_secondary_color }}} 
  #shopify-section-{{ section.id }} .badges_main_color{fill:{{section.settings.badges_main_color }}} 
  
  /* banner container and content */
  #shopify-section-{{ section.id }} {clear:both;z-index:4;position:relative;}  
  #shopify-section-{{ section.id }} #banniere_confiance { {% if slider %}max-height:0px; transition: max-height 0.3s ease-out;overflow-y:hidden;{% endif %}background-color:{{ section.settings.badges_bg_color }};padding:0 25px;margin-top: {{section.settings.badges_space_footer_top_mobile}}px;margin-bottom: {{section.settings.badges_space_footer_mobile}}px;} 
  #shopify-section-{{ section.id }} #banniere_confiance ul{margin: auto;padding:20px 0;max-width: {% if section.settings.max_width == '2000' %}none {% else %}{{ section.settings.max_width }}px{% endif %};} 
  #shopify-section-{{ section.id }} #banniere_confiance ul:not(.slick-initialized) {display: grid;justify-content:center;}
  #shopify-section-{{ section.id }} #banniere_confiance ul li {box-sizing:border-box;width:100%;font-size:{{ section.settings.font_size_mobile }}px;font-weight:600;text-transform:uppercase;padding:5px 0;list-style-type:none;z-index:4;display:flex;align-items:center;outline:none;user-select:none;justify-content:start;{% if section.settings.banner_style_mobile == 'vertical' %}flex-direction:column;gap:12px;text-align:center;margin:0 0 8px 0{% else %}flex-direction:row;margin:0{% endif %}}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-initialized li {justify-content:{{ section.settings.justify_content_mobile }};}
  #shopify-section-{{ section.id }} #banniere_confiance ul li svg, #banniere_confiance ul li img {width:{{ section.settings.badges_size_mobile }}px;height:auto;}
  #shopify-section-{{ section.id }} #banniere_confiance ul li::before {display:none}
  #shopify-section-{{ section.id }} #banniere_confiance ul li span{display: block;line-height: 16px;opacity: 1;} 
  #shopify-section-{{ section.id }} #banniere_confiance ul li:hover,#banniere_confiance ul li:focus{cursor: default;}
  #shopify-section-{{ section.id }} #banniere_confiance ul li .badges-text {margin-left:{% if section.settings.banner_style_mobile == 'vertical' %}0{% else %}15px{% endif %};}
  #shopify-section-{{ section.id }} #banniere_confiance ul li .main-text{color: {{ section.settings.badges_title_color }};}
  #shopify-section-{{ section.id }} #banniere_confiance ul li .hover-text p {color: {{ section.settings.badges_subtitle_color }};text-transform: none;font-weight: normal;margin: 5px 0 0;line-height: 1.25em;padding: inherit;width: 0;opacity: 0;height:0;visibility:hidden;font-size:inherit} 
  #shopify-section-{{ section.id }} #banniere_confiance ul li .hover-text p:not(:first-child) {margin-top:0}
  #shopify-section-{{ section.id }} #banniere_confiance ul li:hover .hover-text p {width: 100%;height:auto;opacity: 1;visibility:visible;}
  #shopify-section-{{ section.id }} #banniere_confiance ul li .hover-text a {text-decoration:underline;color:inherit}
  
  /* slick rules & fixes */
  {% if section.settings.show_dots %}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots {display: flex;flex-direction: row;justify-content: center;bottom: 0;position: relative;padding: 0 0 5px;}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots li {margin: 0 4px;padding: 0;height: 8px;width: 8px;}  
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots li button {background-color:{{section.settings.badges_main_color | color_darken: 20 }};padding: 0;position:absolute;border-radius: 100%;width:6px;height:6px;min-height:6px;opacity:0.3;font-size:0;border:none}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots li button::before{display:none}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots li.slick-active button {width:8px;height: 8px;opacity:1;outline:none}
  #shopify-section-{{ section.id }} #banniere_confiance ul.slick-dots li.slick-active button::before {display:none}
  {% endif %}
  #shopify-section-{{ section.id }} .banniere_confiance-slick {overflow: hidden;}
  #shopify-section-{{ section.id }} .slick-list {width: 100%;}

  
  /* media queries */
  {% if slider %}
  @media (max-width:619px){
    #shopify-section-{{ section.id }}  #banniere_confiance ul li:nth-child(n+1) {display: none;}
    #shopify-section-{{ section.id }} #banniere_confiance ul.slick-initialized li:nth-child(n+1) {display:flex}
  }
  {% endif %}
  @media (min-width:620px){
    #shopify-section-{{ section.id }} #banniere_confiance { {% if section.blocks.size > 4 %}max-height:0px; transition: max-height 0.3s ease-out;overflow-y:hidden;{% else %}max-height:250px;overflow-y:auto;{% endif %} }
    #shopify-section-{{ section.id }} #banniere_confiance ul li {justify-content:center;margin:0;padding:calc(10px + {{section.settings.bg_padding}}px) 15px calc(10px + {{section.settings.bg_padding}}px) 15px;font-size:{{ section.settings.font_size_desk }}px;{% if section.settings.banner_style_desktop == 'vertical' %}flex-direction:column;gap:12px;text-align:center;{% else %}flex-direction:row;gap:0;text-align:left;{% endif %}}
    #shopify-section-{{ section.id }} #banniere_confiance ul li svg, #banniere_confiance ul li img {width:{{ section.settings.badges_size_desk }}px;margin-right: {% if section.settings.show_badges_contour %}5px{% else %}0px{% endif %}}
    #shopify-section-{{ section.id }} #banniere_confiance ul li .badges-text {margin-left:{% if section.settings.banner_style_desktop == 'vertical' %}0{% else %}15px{% endif %};}
  }
  @media (min-width:620px) and (max-width:1275px) {   
    #shopify-section-{{ section.id }}  #banniere_confiance ul li:nth-child(n+4) {display: none;}
    #shopify-section-{{ section.id }} #banniere_confiance ul.slick-initialized li:nth-child(n+4) {display:flex}
    #shopify-section-{{ section.id }} #banniere_confiance ul:not(.slick-initialized) {grid-template-columns: repeat(3, 1fr)}
  }
  @media (min-width:1275px){
    #shopify-section-{{ section.id }} #banniere_confiance {margin-top: {{section.settings.badges_space_footer_top}}px;margin-bottom: {{section.settings.badges_space_footer}}px;}
    #shopify-section-{{ section.id }} #banniere_confiance ul.slick-initialized li {justify-content:{{ section.settings.justify_content_desk }};}
    #shopify-section-{{ section.id }} #banniere_confiance ul li:nth-child(n+{{ section.blocks.size | plus: 1 }}) {display: none;}
    #shopify-section-{{ section.id }} #banniere_confiance ul.slick-initialized li:nth-child(n+{{ section.blocks.size | plus: 1 }}) {display:flex}
    #shopify-section-{{ section.id }} #banniere_confiance ul:not(.slick-initialized) {grid-template-columns: repeat({{ blocks_num }}, 1fr);justify-content:center;}  
  }
  {% if section.settings.show_always_subtitles_desktop == 'normal' %}
  @media screen and (min-width:620px) {#shopify-section-{{ section.id }} #banniere_confiance ul li .hover-text p {width: 100%;height:auto;opacity: 1;visibility:visible;}}
  {% endif %}
  {% if section.settings.show_always_subtitles_mobile == 'normal' %}
  @media screen and (max-width:620px) {#shopify-section-{{ section.id }} #banniere_confiance ul li {padding:10px 0} #shopify-section-{{ section.id }} #banniere_confiance ul li .hover-text p {width: 100%;height:auto;opacity: 1;visibility:visible;}}
  {% endif %} 
  
  /* custom css */
  {{ section.settings.custom_css }}
</style>



