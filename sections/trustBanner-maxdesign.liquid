<script type="lazyload2">
  function sliderTrustBanner_{{ section.id | remove: '-' }}() {
   
    /* slick init setup */
    let banner = document.querySelector('#shopify-section-{{ section.id }} #banniere_confiance')
    let slickBanner = document.querySelector('#shopify-section-{{ section.id }} .banniere_confiance-slick')

    {% if slider or slider == false and section.blocks.size >= 4 %}                        
    /* banner slideDown based on settings */
      jQuery(slickBanner).on('init', function(slick) {
        setTimeout(function() {
        banner.style.maxHeight = '250px';
        banner.style.overflowY = 'auto';
        }, 100)
      })
    {% endif %}
    
    /* update on resize */
    jQuery(window).resize(function(){
      if({{ slider }} == true || window.matchMedia("(min-width: 620px)").matches) {
        banner.style.maxHeight = '250px';
        banner.style.overflowY = 'auto';
        var initResize = true;
        if (initResize) {
          initResize = false;
          if(typeof jQuery.fn.Slick !== 'undefined'){jQuery(slickBanner).slick('unslick')}
          setTimeout(function(){
            jQuery(slickBanner).not('.slick-initialized').slick({
            infinite: !0,
            autoplay: !0,
            autoplaySpeed: {{ section.settings.slider_speed | times: 1000 }},
