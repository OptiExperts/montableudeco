<script type="application/json">
  {
    "cart": {{ cart | json }},
    "cartType": {{ cart_type | json }},
    "cartCountSelector": {{ settings.MD_cart_count_selector | default: '.NA' | json }},
    "cartIndicationSelector": {{ settings.MD_cart_count_indication | split: '/' | first | default: '.NA' | json }},
    "cartIndicationOn": {{ settings.MD_cart_count_indication | split: '/' | last | split: ';' | first | strip | default: 'show' | json }},
    "cartIndicationOff": {{ settings.MD_cart_count_indication | split: '/' | last | split: ';' | last | strip | default: 'hide' | json }},
    "free": {{ TRANSLATIONS.free_text | json }},
    "inventoryAlert": {{ TRANSLATIONS.inventory_alert | json }},
    "appsDisable": {{ settings.MD_apps_disable | json }},
    "moneyFormat": {{ shop.money_format | json }},
    "discount": {
      "auto": {{ automatic_discount | default: false }},
      "qty": {
          "enabled": {{ SAVINGS.qty_discount | default: false }},
          "values": ["{{ qty_discount_values }}"],
          "codes": ["{{ qty_discount_codes }}"],
          "messages": ["{{ qty_discount_messages }}"]
        },
        "qtyManual": {{ SAVINGS.discount_quantity_manual_enable | default: false }}, 
        "custom": {{ SAVINGS.custom_discount | default: false }},
        "promoField": {{ SAVINGS.promofield | default: false }},
        "discounts": { "10" : ["{{ SAVINGS.discount_code_10 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "15" : ["{{ SAVINGS.discount_code_15 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "20" : ["{{ SAVINGS.discount_code_20 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "25" : ["{{ SAVINGS.discount_code_25 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "30" : ["{{ SAVINGS.discount_code_30 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "35" : ["{{ SAVINGS.discount_code_35 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "40" : ["{{ SAVINGS.discount_code_40 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "45" : ["{{ SAVINGS.discount_code_45 | strip | replace: ', ', ',' | replace: ',', '","' }}"], "50" : ["{{ SAVINGS.discount_code_50 | strip | replace: ', ', ',' | replace: ',', '","' }}"]}
      },
      "announce" : {
        "enabled": {{ BAR.enable | default: false }},
        "text": {{ BAR.message_text | json }}
      },
      "offer" : {
        "type": {{ OFFER.threshold_type | json }},
        "enable": {{ offer_threshold | default: false }},
        "qualify": {{ OFFER.qualify_text | json }},
        "product": {
          "id": {{ product_ID  | default: false }},
          "prop": {"_upsell_maxdesign": "Bought with cart threshold upsell", "_url_original": "{% if OFFER.hide_qty %}{{ all_products[OFFER.product_upsell_original].url | default: '#' }}{% else %}{{ all_products[OFFER.product_upsell_original].url | default: all_products[OFFER.product_upsell].url }}{% endif %};{{ OFFER.hide_qty }}" {% if OFFER.product_original_property != blank %}, "{{ upsell_first }}": "{{ upsell_last }}"{% endif %} },
          "thresholdValue": {{ product_threshold_value | remove: '.0' | default: 0 }},
          "text": {{ OFFER.product_text | json }},
          "icon": {{ product_icon | json }}
        },
        "shipping": {
          "defaultPrice": {{ delivery_price_value | default: 0 }},
          "priceBefore": {{ OFFER.delivery_price_text_before | json }},
          "priceAfter": {{ OFFER.delivery_price_text_after | json }},
          "thresholdValue": {{ shipping_threshold_value | remove: '.0' | default: 0 }},
          "text": {{ OFFER.shipping_text | json }},
          "icon": {{ shipping_icon | json }}
        },
        "both": {
          "step_1": {{ primary_threshold | json }},
          "step_2": {{ secondary_threshold | json }},
          "iconProgress": {{ offer_icon_progress | json }}
        }
      },
      "cdTimer": {
        "enabled": {% if TIMER %}true{% else %}false{% endif %},
        "time": {{ TIMER.cart_timer_time | default: 0 }},
        "expiry": {{ TIMER.cart_timer_renew | default: 0 }}
      }
    }
  </script>
</cart-maxdesign>



