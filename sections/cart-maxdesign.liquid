{%- liquid
  comment 
    #pragma THEME SETTINGS
  endcomment
  if settings.MD_cart_type == 'cart' or template.suffix == nil
    assign cart_type = 'cart'
  elsif settings.MD_cart_type == 'drawer'
    assign cart_type = 'drawer'
  endif
  comment 
    #pragma ASSIGN BLOCK SETTINGS
  endcomment
  for block in section.blocks
    case block.type
      when 'bg' 
        assign COLORS = section.blocks[forloop.index0]
        assign COLORS = COLORS.settings
      when 'badges'
        assign BADGES = section.blocks[forloop.index0]
        assign BADGES = BADGES.settings
      when 'savings'
        assign SAVINGS = section.blocks[forloop.index0]
        assign SAVINGS = SAVINGS.settings
      when 'timer'
        assign TIMER = section.blocks[forloop.index0]
        assign TIMER = TIMER.settings
      when 'navigation'
        assign NAVIGATION = section.blocks[forloop.index0]
        assign NAVIGATION = NAVIGATION.settings
      when 'announce'
        assign BAR = section.blocks[forloop.index0]
        assign BAR = BAR.settings
      when 'offer'
        assign OFFER = section.blocks[forloop.index0]
        assign OFFER = OFFER.settings
      when 'translations'
        assign TRANSLATIONS = section.blocks[forloop.index0]
        assign TRANSLATIONS = TRANSLATIONS.settings
      when 'attribute'
        assign ATTRIBUTE = section.blocks[forloop.index0]
        assign ATTRIBUTE = ATTRIBUTE.settings
    endcase
  endfor
  comment 
    #pragma LIQUID UTILITIES
  endcomment
  assign breakPoint = settings.MD_cart_query
  assign font_size_desktop = settings.MD_font_size_desktop | divided_by: 100.0 | plus: 1
  assign font_size_drawer_desktop = settings.MD_font_size_drawer_desktop | divided_by: 100.0 | plus: 1
  assign font_size_mobile = settings.MD_font_size_mobile | divided_by: 100.0 | plus: 1
  assign btn_brightness = COLORS.button_bg_color | color_brightness | plus: 0 | round
  if btn_brightness > 40
    assign btn_color_hover = COLORS.button_bg_color | color_darken: 10 
  else
    assign btn_color_hover = COLORS.button_bg_color | color_lighten: 15
  endif
  assign wrapper_opacity_mobile = COLORS.bg_secondary_opacity | divided_by: 100.0
  assign wrapper_opacity_desktop = COLORS.bg_secondary_opacity_desktop | divided_by: 100.0
  comment 
    #pragma CHECK FREE DELIVERY
  endcomment
  if section.settings.delivery_enable
    assign delivery_enable = true
  endif
  comment 
    #pragma CHECK PRODUCT / SHIPPING DISCOUNT THRESHOLD
  endcomment
  if OFFER.threshold_type != 'disable'
    assign offer_threshold = true

    assign product_ID = all_products[OFFER.product_upsell].selected_or_first_available_variant.id
    assign product_threshold_value = OFFER.product_threshold_value | times: 100.0
    assign product_goal_to_threshold = product_threshold_value | minus: cart.original_total_price | money | prepend: '<span>' | append: '</span>'
    assign product_current_threshold = product_threshold_value | money_without_trailing_zeros | strip_html
    if cart.original_total_price >= product_threshold_value
      assign product_upsell_qualify = true
    endif
    if OFFER.product_original_property contains ':'
      assign upsell_first = OFFER.product_original_property | split: ':' | first
      assign upsell_last = OFFER.product_original_property | split: ':' | last
    else
      assign upsell_first = 'Promo'
      assign upsell_last = OFFER.product_original_property
    endif
    if OFFER.product_icon != blank
      assign product_icon = 'attr(data-icon-product)'
    else
      assign product_icon = 'attr()'
    endif

    assign shipping_threshold_value = OFFER.shipping_threshold_value | times: 100.0
    assign shipping_goal_to_threshold = shipping_threshold_value | minus: cart.original_total_price | money | prepend: '<span>' | append: '</span>'
    assign shipping_current_threshold = shipping_threshold_value | money_without_trailing_zeros | strip_html
    if cart.original_total_price >= shipping_threshold_value
      assign shipping_upsell_qualify = true
    endif
    if OFFER.shipping_icon != blank
      assign shipping_icon = 'attr(data-icon-shipping)'
    else
      assign shipping_icon = 'attr()'
    endif

    if OFFER.threshold_type == 'shipping'
      if shipping_upsell_qualify
        assign offer_qualify = true
        assign offer_text = OFFER.qualify_text
      else
        assign offer_text = OFFER.shipping_text
        assign offer_goal_to_threshold = shipping_goal_to_threshold
        assign offer_current_threshold = shipping_current_threshold
        assign offer_icon_after = shipping_icon
        assign offer_bar_progress = cart.original_total_price | times: 100.0 | divided_by: shipping_threshold_value
      endif
    elsif OFFER.threshold_type == 'product'
      if product_upsell_qualify
        assign offer_qualify = true
        assign offer_text = OFFER.qualify_text
      else
        assign offer_text = OFFER.product_text
        assign offer_goal_to_threshold = product_goal_to_threshold
        assign offer_current_threshold = product_current_threshold
        assign offer_icon_after = product_icon
        assign offer_bar_progress = cart.original_total_price | times: 100.0 | divided_by: product_threshold_value
      endif
    else
      if shipping_threshold_value < product_threshold_value
        if product_upsell_qualify
          assign offer_qualify = true
          assign offer_text = OFFER.qualify_text
        elsif shipping_upsell_qualify
          assign offer_text = OFFER.product_text
          assign offer_goal_to_threshold = product_goal_to_threshold
          assign offer_current_threshold = product_current_threshold
        else
          assign offer_text = OFFER.shipping_text
          assign offer_goal_to_threshold = shipping_goal_to_threshold
          assign offer_current_threshold = shipping_current_threshold
          endif
        assign primary_threshold = 'shipping'
        assign secondary_threshold = 'product'
        assign offer_icon_before = shipping_icon
        assign offer_icon_after = product_icon
        assign offer_icon_progress = shipping_threshold_value | divided_by: OFFER.product_threshold_value
        assign offer_bar_progress = cart.original_total_price | times: 100.0 | divided_by: product_threshold_value
      else
        if shipping_upsell_qualify
          assign offer_qualify = true
          assign offer_text = OFFER.qualify_text
        elsif product_upsell_qualify
          assign offer_text = OFFER.shipping_text
          assign offer_goal_to_threshold = shipping_goal_to_threshold
          assign offer_current_threshold = shipping_current_threshold
        else
          assign offer_text = OFFER.product_text
          assign offer_goal_to_threshold = product_goal_to_threshold
          assign offer_current_threshold = product_current_threshold
        endif
        assign primary_threshold = 'product'
        assign secondary_threshold = 'shipping'
        assign offer_icon_before = product_icon
        assign offer_icon_after = shipping_icon
        assign offer_icon_progress = product_threshold_value | divided_by: OFFER.shipping_threshold_value
        assign offer_bar_progress = cart.original_total_price | times: 100.0 | divided_by: shipping_threshold_value
      endif
      assign offer_bar_icon_diff = offer_bar_progress | minus: offer_icon_progress
      if offer_bar_icon_diff > 0 and offer_bar_icon_diff < 15
        assign offset_bar_price = true
      endif 
    endif
  endif
  comment
    #pragma CHECK AUTOMATIC DISCOUNT
  endcomment
  if cart.total_price != cart.original_total_price
    assign automatic_discount = true
  endif
-%}

{%- comment -%}
  #pragma CHECK QUANTITY DISCOUNT
{%- endcomment -%}
{%- if SAVINGS.qty_discount -%}
  {%- assign qty_discount_index = cart.item_count | minus: 1 | at_least: 0 | at_most: 3 -%}
  {%- capture qty_discount_codes -%}","{{ SAVINGS.discount_quantity_2 | split: '/' | first | strip }}","{{ SAVINGS.discount_quantity_3 | split: '/' | first | strip }}","{{ SAVINGS.discount_quantity_max | split: '/' | first | strip }}{%- endcapture -%}{%- assign qty_discount_codes_array = qty_discount_codes | split: '","' -%}
  {%- capture qty_discount_values -%}","{{ SAVINGS.discount_quantity_2 | split: '/' | last | remove: '%' | strip }}","{{ SAVINGS.discount_quantity_3 | split: '/' | last | remove: '%' | strip }}","{{ SAVINGS.discount_quantity_max | split: '/' | last | remove: '%' | strip }}{%- endcapture -%}{%- assign qty_discount_values_array = qty_discount_values | split: '","' -%}
  {%- capture qty_discount_messages -%}","{{ SAVINGS.cart_message_text_discount_quantity_2 }}","{{ SAVINGS.cart_message_text_discount_quantity_3 }}","{{ SAVINGS.cart_message_text_discount_quantity_max }}{%- endcapture -%}{%- assign qty_discount_messages_array = qty_discount_messages | split: '","' -%}
  {%- assign qty_discount_code = qty_discount_codes_array[qty_discount_index] -%}
  {%- assign qty_discount_value = cart.original_total_price | times: qty_discount_values_array[qty_discount_index] | divided_by: 100 -%}
  {%- assign qty_discount_message = qty_discount_messages_array[qty_discount_index] -%}
  {%- if qty_discount_codes_array[qty_discount_index] != blank -%}{%- assign qty_discount_active = true -%}{%- endif -%}
{%- endif -%}

{%- liquid
  comment
    #pragma DISCOUNT & DISCOUNTED TOTAL
  endcomment
  capture cart_discount
    if automatic_discount
      echo cart.original_total_price | minus: cart.total_price
    else
      echo qty_discount_value
    endif
  endcapture
  capture cart_total
    if automatic_discount
      echo cart.total_price
    else
      echo cart.original_total_price | minus: qty_discount_value
    endif
  endcapture
  comment
    #pragma DELIVERY PRICE
  endcomment
  if delivery_enable
    if OFFER.threshold_type == 'shipping' or OFFER.threshold_type == 'both'
      if shipping_upsell_qualify
        if OFFER.delivery_price_text_after contains '.' or OFFER.delivery_price_text_after contains ','
          assign delivery_price_text = OFFER.delivery_price_text_after | remove: '.' | remove: ',' | money
          assign delivery_price_value = OFFER.delivery_price_text_after | remove: '.' | remove: ','
        else
          assign delivery_price_text = OFFER.delivery_price_text_after
          assign delivery_price_value = 0
        endif
      else
        if OFFER.delivery_price_text_before contains '.' or OFFER.delivery_price_text_before contains ','
          assign delivery_price_text = OFFER.delivery_price_text_before | remove: '.' | remove: ',' | money
          assign delivery_price_value = OFFER.delivery_price_text_before | remove: '.' | remove: ','
        else
          assign delivery_price_text = OFFER.delivery_price_text_before
          assign delivery_price_value = 0
        endif
      endif
    elsif TRANSLATIONS.delivery_price_text contains '.' or TRANSLATIONS.delivery_price_text contains ','
      assign delivery_price_text = TRANSLATIONS.delivery_price_text | remove: '.' | remove: ',' | money
      assign delivery_price_value = TRANSLATIONS.delivery_price_text | remove: '.' | remove: ','
    else
      assign delivery_price_text = TRANSLATIONS.delivery_price_text
      assign delivery_price_value = 0
    endif
  endif
-%}



{%- comment -%} 
  #pragma LIQUID STYLES VARIABLES
{%- endcomment -%}

{%- style -%}
  /* VARIABLES
  ========================= */
  #shopify-section-{{ section.id }} {
    --header-color: {{ NAVIGATION.header_color }};

    --message-text-color: {{ BAR.message_color }};
    --message-bg-color: {{ BAR.message_bgcolor }};

    --stepper-bg-color-mobile: {{ NAVIGATION.stepper_bg_color_mobile }};
    --stepper-main-color-mobile: {{ NAVIGATION.stepper_main_color }};
    --stepper-secondary-color-mobile: {{ NAVIGATION.stepper_secondary_color }};
    --stepper-secondary-color-mobile-lighten: {{ NAVIGATION.stepper_secondary_color | color_lighten: 50 }};
    --stepper-bg-color-desktop: {{ NAVIGATION.stepper_bg_color_desktop }};

    --upsell-order: {{ OFFER.mobile_order }};
    --upsell-bar-color1: {{ OFFER.upsell_bar_color_1 }};
    --upsell-bar-color2: {{ OFFER.upsell_bar_color_2 }};
    --upsell-bar-bg-color: #f3f3f3;
    --upsell-bar-display: {% if offer_qualify %}none{% else %}flex{% endif %};
    --upsell-icon-progress: {{ offer_icon_progress }}%;
    --upsell-icon-step1: {{ offer_icon_before }}; 
    --upsell-icon-step2: {{ offer_icon_after }};
    --upsell-price-transition: 0.4s 0.3s;

    --labels-text-color-desktop: {{ COLORS.labels_text_color_desktop }};
    --labels-bg-color-alpha-desktop: {{ COLORS.labels_bg_color_desktop | color_modify: 'alpha', wrapper_opacity_desktop }};

    --cart-primary-color-mobile: {{ COLORS.primary_color_mobile }};
    --cart-primary-color-lighten-mobile: {{ COLORS.primary_color_mobile | color_lighten: 25 }};
    --cart-primary-color-desktop: {{ COLORS.primary_color_desktop }};
    --border-color-mobile: {{ COLORS.borders_color_mobile }};
    --border-radius-desktop: {{ COLORS.borders_radius_desktop }}px;
    --border-color-desktop: {{ COLORS.borders_color_desktop }};

    --bg-color-mobile: {{ COLORS.bg_color }};
    --bg-pannels-bottom-color-alpha-mobile: {{ COLORS.bg_pannels_bottom_color_mobile | color_modify: 'alpha', wrapper_opacity_mobile }};
    --bg-desktop: {% if COLORS.bg_img_desktop == blank %}{{ COLORS.bg_color_desktop }}{% else %}url('{{ COLORS.bg_img_desktop | img_url: '1400x', format: 'pjpg' }}'){% endif %};
    --bg-pannels-left-color-alpha-darken-desktop: {{ COLORS.bg_pannels_left_color_desktop | color_darken:10 | color_modify: 'alpha', wrapper_opacity_desktop }};
    --bg-opacity-desktop: {{ COLORS.bg_opacity_desktop | divided_by: 100.0 }};
    --bg-pos-desktop: {{ COLORS.bg_poshorizontal_desktop }}% {{ COLORS.bg_posvertical_desktop}}%;
    --bg-pannels-left-color-alpha-desktop: {{ COLORS.bg_pannels_left_color_desktop | color_modify: 'alpha', wrapper_opacity_desktop }};
    --bg-pannels-right-color-alpha-desktop:{{ COLORS.bg_pannels_right_color_desktop | color_modify: 'alpha', wrapper_opacity_desktop }};	

    --flash-text-color: {{ COLORS.flash_text_color }};
    --total-text-color: {{ COLORS.total_text_color }};
    --total-subtext-color: {{ COLORS.total_subtext_color }};
    --total-price-color: {{ COLORS.total_price_color }};
    --savings-text-color: {{ SAVINGS.savings_color }};

    --discount-bg-color: {{ SAVINGS.discount_bg_color }};
    --discount-bg-padding: {% if SAVINGS.discount_bg_color == nil %}4px 5px 4px 0{% else %}4px 5px{% endif %};
    --discount-text-color: {{ SAVINGS.discount_text_color }};
    --discount-applied-margin: {% if delivery_enable %}15px 0 -5px{% else %}15px 0{% endif %};
    --discount-field-color: {{ SAVINGS.promofield_color }};

    --timer-color1: {{ TIMER.cart_timer_color1 }};
    --timer-color2: {{ TIMER.cart_timer_color2 }};

    --badges-text-color: {{ BADGES.color }};
    --continue-shopping-color:{{ NAVIGATION.continue_shopping_color }};

    --button-height: {{ section.settings.button_height }}px;
    --button-radius: {{ section.settings.button_radius }}px;
    --express-button-min-height: {{ section.settings.button_radius | plus: 12 }}px;
    --button-text-color: {{ COLORS.button_text_color }};
    --button-bg-color: {{ COLORS.button_bg_color }};
    --button-bg-color-hover: {{ btn_color_hover }};

    --img-width-mobile: {{ 95 | times: font_size_mobile }}px;

    --font-size-mobile: {{ font_size_mobile }}em;
    --font-size-desktop: {{ font_size_desktop }}em;
    --font-size-drawer-desktop: {{ font_size_drawer_desktop }}em;
  }
  /* RESETS
  ========================= */
  {% if template.suffix == nil %}
    body, html, .page-container {background:none}
    body .main-content.main-content, main, .main, main .container, main .wrapper, .page-container {padding:0!important;margin:0!important;max-width:none;width:auto}
    header .sticky, header .sticky--active, header.sticky, header.sticky--active {position: relative}
    footer {margin-top:0!important}
  {% endif %}
  /* CUSTOM CSS
  ========================= */
  @media (min-width:{{ breakPoint }}px) { {{ settings.MD_cart_css_desktop }} }
  @media (max-width:{{ breakPoint | minus: 1 }}px) { {{ settings.MD_cart_css_mobile }} }
{%- endstyle -%}



{%- comment -%}
   #pragma MARKUP
{%- endcomment -%}

<cart-maxdesign>
  {%- if cart.item_count > 0 -%}

    {%- comment -%} 
      #pragma CART MESSAGE
    {%- endcomment -%}

    {%- if SAVINGS.qty_discount and automatic_discount == nil and qty_discount_messages_array[qty_discount_index] != blank -%}
      <div class="cartMessage__wrapper">
        <div class="cartMessage__text">{{ qty_discount_messages_array[qty_discount_index] }}</div>
      </div>
    {%- elsif BAR.message_text != blank  -%}
      <div class="cartMessage__wrapper {% unless BAR.enable %}cartMessage__wrapper--hidden{% endunless %}">
        <div class="cartMessage__text">{{ BAR.message_text }}</div>
      </div>
    {%- endif -%}
  
    {%- comment -%}
      #pragma CART STEPPER
    {%- endcomment -%}

    {%- if NAVIGATION.stepper_enable -%}
      <div class="cart__steps--wrapper">
        <ul class="cart__steps">
          {%- for i in (1..4) -%}
            {%- capture step -%}stepper_step{{ i }}_text{%- endcapture -%}
            <li {% if forloop.first %}class="cart__steps--completed"{% endif %}>
              <span>{{ NAVIGATION[step] }}</span>
            </li>
            {%- unless forloop.last -%}
              <li><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 10 10"><path d="M2 1l1-1 4 4 1 1-1 1-4 4-1-1 4-4"></path></svg></li>
            {%- endunless -%}
          {%- endfor -%}
        </ul>
      </div>
    {%- endif -%}
  
  	{%- comment -%}
      #pragma DRAWER HEADER
    {%- endcomment -%}
  
    <h1>
      <span>{{ NAVIGATION.header_text }}</span>
      <span class="cart__close">
        <svg viewBox="0 0 13 13"><path d="M1.5 1.5l10.05 10.05M11.5 1.5L1.45 11.55" stroke-width="2" fill="none" fill-rule="evenodd" stroke-linecap="round"></path></svg>
      </span>
    </h1>
  
  	{%- comment -%}
      #pragma CUSTOM CONTENT - TOP POSITION 
    {%- endcomment -%}
  
    {%- for block in section.blocks -%}
      {%- if block.settings.block_pos == 'top' -%}
  	  	{%- if block.type == 'custom' -%}
          <div class="custom__text" style="--custom-text-color:{{ block.settings.custom_content_color }}">
            {{ block.settings.custom_content }}
          </div>
  		  {%- elsif block.type == 'timer' -%}
          <div id="cdTimerCart">
            <p>{{ TIMER.cart_timer_innerText | replace: 'XX', '<span class="cdTimerCart--color"><span id="cart_minutes">&nbsp;&nbsp;</span>:<span id="cart_seconds">&nbsp;&nbsp;</span></span>' }}</p>
          </div>
  		  {%- endif -%}
  		  <hr class="summary__divider" style="margin:10px auto 0;width:calc(100% - 40px);max-width:700px;">
      {%- endif -%}
    {%- endfor -%}
  
  	{%- comment -%}
      CART FORM WRAPPER 
    {%- endcomment -%}
  
    <div class="cartForm__wrapper">
      <form name="cartForm" action="/cart" method="POST">

        {%- comment -%} 
          #pragma SHIPPING / PRODUCT UPSELL
        {%- endcomment -%}

        {%- if offer_threshold -%}
          <div class="upsell-threshold__wrapper {% if OFFER.mobile_order == '1' %}upsell-threshold__wrapper--top{% endif %}">
            <div class="upsell-threshold__text">
              {{- offer_text | replace: 'XX', offer_goal_to_threshold | replace: 'YY', offer_current_threshold -}}
            </div>
            <div class="upsell-threshold__bar--container">
              <div class="upsell-threshold__bar" data-icon-shipping="{{ OFFER.shipping_icon | slice: 0 }}" data-icon-product="{{ OFFER.product_icon | slice: 0 }}">
                <span 
                  class="upsell-threshold__progress {% if offset_bar_price %}upsell-threshold__progress--offset{% endif %}" 
                  data-progress-min="{{ 0 | money_without_trailing_zeros | strip_html }}" 
                  data-progress="{{ cart.original_total_price | money_without_trailing_zeros | strip_html }}" 
                  style="width:{{ offer_bar_progress }}%">
                </span>
                {%- if OFFER.info_text != blank -%}
                  <span class="upsell-threshold__details--trait"></span>
                {%- endif -%}
              </div>
              <span style="font-size:12px;margin-left:6px;">
                {% if OFFER.threshold_type != 'both' %}
                  {{ offer_current_threshold }}
                {%- else -%}
                  {{ shipping_threshold_value | at_least: product_threshold_value | money_without_trailing_zeros | strip_html }}
                {%- endif -%}
              </span>
            </div>
            {%- if OFFER.info_text != blank -%}
              <div class="upsell-threshold__details">{{ OFFER.info_text }}</div>
            {%- endif -%}
            <hr class="summary__divider mobile__content">
          </div>
        {%- endif -%}

        {%- comment -%}
          #pragma CART ITEMS CONTAINER
        {%- endcomment -%}

        <div class="cartItems__wrapper {% if section.settings.labels_enable %}cartItems__wrapper--labels{% endif %}">
        {%- for item in cart.items -%}

          {%- liquid
            comment
              RESET LOOP VARIABLES
            endcomment
            assign lastloop = forloop.last
            assign original_item_url = item.url
            assign hide_quantities = false
            assign enable_compare_at_price = false
            assign properties_count = 0
            assign discount_property = nil
            assign is_mobile_absolute = false
            comment 
              #pragma MANUAL QTY BREAKS OFFER
            endcomment
            assign qty_offer = false
            if item.product.options_by_name['discount'] != blank
              assign qty_offer = true
              assign option_discounts = item.product.options_by_name['discount'].values | join: ','
              assign option_position = item.product.options_by_name['discount'].position | minus: 1
            endif
            comment
              #pragma GET COMPARE AT PRICE SAVINGS
            endcomment
            if SAVINGS.enable
              if SAVINGS.compare_price_enable
                if item.variant.compare_at_price > item.variant.price
                  assign total_saving = item.variant.compare_at_price | minus: item.variant.price | times: item.quantity | plus: total_saving
                endif
              elsif discounted_item_id == item.id
                assign total_saving = item.variant.compare_at_price | minus: item.variant.price | times: item.quantity | plus: total_saving
              endif
            endif
            comment
              #pragma HIDE / SHOW CART ATTRIBUTE FOR SOME PRODUCTS
            endcomment
            if ATTRIBUTE
              if ATTRIBUTE.collection == blank
                assign show_cart_attribute = true
              else
                for product in collections[ATTRIBUTE.collection].products
                  if item.product_id == product.id
                    assign show_cart_attribute = true
                    break
                  endif
                endfor
              endif
            endif
          -%}

          {%- comment -%}
            #pragma HANDLE LINE ITEM PROPERTIES
          {%- endcomment -%}
        
          {%- capture item_properties -%}
            {%- for p in item.properties -%}
              {%- assign first_character_in_key = p.first | truncate: 1, '' -%}
              {%- if enable_compare_at_price -%}
                {%- capture discount_property -%}
                  {%- assign p_size = p.first.size | plus: p.last.size -%}
                  {%- if p_size > 35 -%}
                    {%- assign is_mobile_absolute = true -%}
                  {%- endif -%}
                  <span class="cartItems__prices--discount {% if is_mobile_absolute %}cartItems__prices--discount-mobile-absolute{% endif %} {% if SAVINGS.discount_bg_color != 'rgba(0,0,0,0)' %}cartItems__prices--discount-color{% endif %}">
                    <li>{{ p.first | remove: ':' }} : {{ p.last }}</li>
                  </span>
                {%- endcapture -%}
              {%- elsif p.last != blank and first_character_in_key != '_' -%}
                {% assign properties_count = properties_count | plus: 1 %}
                <p class="cartItems__properties">
                  <span class="cartItems__properties--first">{{ p.first | remove: ':' }} : </span>
                  <span class="cartItems__properties--last">
                    {% if p.last contains '/uploads/' %}
                      <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                    {% else %}
                      {{ p.last }}
                    {% endif %}
                  </span>
                </p>
              {% elsif p.first == '_url_original' %}
                {%- assign original_item_url = p.last | split: ';' | first -%}
                {%- assign enable_compare_at_price = true -%}
                {%- assign is_hide_quantities = p.last | split: ';' | last -%}
                {%- if is_hide_quantities == 'true' -%}
                  {%- assign hide_quantities = true -%}
                {%- endif -%}
                {% assign discounted_item_id = item.id %}
              {%- endif -%}
            {%- endfor -%}
          {%- endcapture -%}
        
          {%- comment -%} 
            #pragma REMOVE ITEM WITH CONDITIONS
            > if last item with discount property or hidden type
          {%- endcomment -%}
        
          {%- if cart.item_count == 1 and discount_property != nil or item.product.type == 'hidden' -%}
            {%- style -%}.btnCart {background-color:#c8c8c8;cursor:default;pointer-events:none}{%- endstyle -%}
            <script type="module">MaxDesign.cart.changeCart(0, '{{ forloop.index }}')</script>
          {%- endif -%}
        
          {%- comment -%} 
            #pragma CHANGE QTY POSITION IN MOBILE WHEN MULTIPLE PROPERTIES
          {%- endcomment -%}
        
          {%- if properties_count > 0 -%}
            {% style %}#shopify-section-{{ section.id }} .cartItems__container[data-item-line="{{ forloop.index }}"] .cartItems__quantity {top:calc(var(--img-width-mobile) - 12px)}{% endstyle %}
          {%- endif -%}

          {%- comment -%} 
            #pragma ENABLE COMPARE PRICE WHEN ITEM SHOPIFY DISCOUNT
          {%- endcomment -%}

          {%- if item.line_level_discount_allocations != nil -%}
            {%- assign enable_compare_at_price = true -%}
            {%- if item.message > 35 or item.product.variants.size > 0 -%}
              {%- assign is_mobile_absolute = true -%}
            {%- endif -%}
          {%- endif -%}
          
          {%- comment -%} 
            #pragma CAPTURE MARKUP CLASSES & ATTRIBUTES
          {%- endcomment -%}

          {%- capture items_container_classes -%}
            {%- if enable_compare_at_price -%}{%- if lastloop %} discounted-lastRow{%- elsif is_mobile_absolute %} discounted{%- endif -%}{%- else -%}{%- for discount in item.discounts -%}{%- if lastloop %} discounted-lastRow{%- elsif discount.title.size > 30 %} discounted{%- endif -%}{%- endfor -%}{%- endif -%}
          {%- endcapture -%}
          {%- capture items_container_attributes -%}
            data-item-line="{{ forloop.index }}" data-item-id="{{ item.variant.id }}" data-item-qty="{{ item.quantity }}" {% if qty_offer %}data-offer-id="{{ item.product.id }}" data-item-discounts="{{ option_discounts }}" data-discount-position="{{ option_position }}" data-discount-value="{{ item.variant.options[option_position] }}"{% endif %}
          {%- endcapture -%}
        
          {%- comment -%}
            #pragma LABELS ON CART PAGE
          {%- endcomment -%}
        
          {%- if forloop.first and section.settings.labels_enable -%}
            <div class="cartItems__labels">
              <span class="cartItems__labels--product">{{ TRANSLATIONS.product_text }}</span>
              <span class="cartItems__labels--qty">{{ TRANSLATIONS.qty_text }}</span>
              <span class="cartItems__labels--price">
                <span>{{ TRANSLATIONS.price_text }}</span>
                <span class="cartItems__labels--price--hidden">{{ item.line_price | money }}</span>
              </span>
            </div>
          {%- endif -%} 
        
          {%- comment -%}
            #pragma ITEMS CONTAINER
          {%- endcomment -%}
        
          <div class="cartItems__container {{- items_container_classes -}}" {{ items_container_attributes }}>          
            <div class="cartItems__image">
              <a href="{% if section.settings.product_disable_link %}javascript:void(0){% else %}{{ original_item_url }}{% endif %}">
                <img loading="lazy" src="{{ item | image_url: width: 200 }}" {% if COLORS.img_border %}class="cartItems__image--border"{% endif %} alt="{{ item.title | escape }}">
              </a>
            </div>
            <div class="cartItems__infos {% if TRANSLATIONS.flash_text != blank or item.product.metafields.cart.cart_info != blank %}cartItems__infos--space-between{% endif %}">
              <div class="cartItems__title {% unless item.variant.title contains 'Default' %}cartItems__title--height{% endunless %}">
                <a href="{% if section.settings.product_disable_link %}#{% else %}{{ original_item_url }}{% endif %}" {% if hide_quantities %}style="padding-right:0"{% endif %}>
                  <span {% if section.settings.ellipsis %}class="cartItems__title--ellipsis" title="{{ item.product.title }}"{% endif %}>{{ item.product.title }}</span>
                  {% unless hide_quantities %}
                    <span class="cartItems__close mobile__content drawer__content">×</span>
                  {% endunless %}
                </a>
                {% unless item.variant.title contains 'Default' %}
                  <p>{{ item.variant.title }}</p>
                {% endunless %}
                {% for discount in item.discounts %}
                  {% if SAVINGS.discount_text_enable %}
                    <span class="cartItems__prices--discount {% if discount.title.size > 30 %}cartItems__prices--discount-mobile-absolute{% endif %}">
                      <li>{{ discount.title }}</li>
                    </span>
                  {% endif %}
                {% endfor %}
                {{ discount_property }}
                {% if item.product.metafields.cart.cart_info != blank or TRANSLATIONS.flash_text != blank and enable_compare_at_price == false %}
                <div class="flash__text {% if item.product.metafields.cart.cart_info_blink == 1 %}flash__text--blinking{% endif %}">
                  <p>{{ item.product.metafields.cart.cart_info | default: TRANSLATIONS.flash_text }}</p>
                </div>
                {% endif %}
              </div>
              <div class="cartItems__quantity" {% if hide_quantities %}disabled{% endif %}>
                <div class="cartItems__quantity--controls">
                  <button {% if hide_quantities %} disabled {% endif %} type="button" data-decrement>−</button>
                  <input {% if hide_quantities %} disabled {% endif %} readonly name="updates[]" class="quantity-selector" value="{{ item.quantity }}" min="0">
                  <button {% if hide_quantities %} disabled {% endif %} type="button" data-increment {% if item.variant.inventory_management != nil and item.variant.inventory_policy != 'continue' %}data-quantity-max="{{ item.variant.inventory_quantity }}"{% endif %}>＋</button>
                </div>
                <select {% if hide_quantities %} disabled {% endif %} class="quantity-dropdown" name="updates[]" {% if item.variant.inventory_management != nil and item.variant.inventory_policy != 'continue' %}data-quantity-max="{{ item.variant.inventory_quantity }}"{% endif %} value="{{ item.quantity }}">
                  {% for i in (0..10) %}
                  <option {% if i == item.quantity %}selected="selected"{% endif %} value="{{ i }}">{{ i }}</option>
                  {% endfor %}
                </select>   
                <svg class="cartItems__bin desktop__content" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 15.813 17"><defs/><defs/><path d="M15.369 2.308h-4.784v-.826A1.473 1.473 0 009.103 0H6.478a1.473 1.473 0 00-1.482 1.482v.826H.423A.424.424 0 000 2.731a.424.424 0 00.423.423h1.058l.931 12.279A1.706 1.706 0 004.106 17h7.389a1.69 1.69 0 001.694-1.567l.931-12.279h1.27a.424.424 0 00.423-.423.442.442 0 00-.444-.423zm-9.527-.826a.624.624 0 01.635-.635h2.646a.624.624 0 01.635.635v.826H5.842zm6.479 13.868a.852.852 0 01-.847.784H4.106a.852.852 0 01-.847-.784L2.328 3.133h10.945z"/><path d="M7.79 5.339a.424.424 0 00-.423.423v7.812a.424.424 0 00.423.423.424.424 0 00.423-.423V5.759a.425.425 0 00-.423-.42zM10.119 5.739l-.423 7.81a.425.425 0 00.4.445h.021a.42.42 0 00.423-.4l.423-7.812a.425.425 0 00-.4-.445.439.439 0 00-.444.4zM5.016 5.339a.44.44 0 00-.4.445l.423 7.812a.42.42 0 00.423.4h.021a.44.44 0 00.4-.445l-.422-7.812a.438.438 0 00-.444-.4z"/></svg>
              </div>
              <div class="cartItems__prices--container">
                <div class="cartItems__prices">
                  <div class="cartItems__prices-total" data-price="{{ item.final_price }}">
                    {% unless item.final_line_price == 0 %}{{ item.final_line_price | money }}{% else %}{{ TRANSLATIONS.free_text }}{% endunless %}
                  </div>
                  {%- if item.unit_price_measurement -%}
                  <span class="cartItems__prices-unit">
                    {%- capture unit_price_base_unit -%}
                      {%- if item.unit_price_measurement.reference_value != 1 -%}
                        {{- item.unit_price_measurement.reference_value -}}
                      {%- endif -%}
                      {{- item.unit_price_measurement.reference_unit -}}
                    {% endcapture %}
                    <span data-unit-price>({{ item.unit_price | money }}</span>/{{ unit_price_base_unit }})
                  </span>
                  {%- endif -%}
                  {%- if SAVINGS.compare_price_enable -%}
                    {%- if item.variant.compare_at_price > item.variant.price or item.line_level_total_discount > 0 -%}
                      {%- assign item_compare_price = item.variant.compare_at_price | default: item.original_price -%}
                      <del class="cartItems__prices--compare" data-compare-price="{{ item_compare_price }}">{{ item_compare_price | times: item.quantity | money }}</del>
                    {%- endif -%}
                  {%- elsif enable_compare_at_price -%}
                    {%- if item.line_level_total_discount > 0 -%}
                      {%- assign item_compare_price = item.original_price -%}
                    {%- else -%}
                      {%- assign item_compare_price = item.variant.compare_at_price -%}
                    {%- endif -%}
                    <del class="cartItems__prices--compare" data-compare-price="{{ item_compare_price }}">{{ item_compare_price | times: item.quantity | money }}</del>
                  {%- endif -%}
                </div>
              </div>
            </div>
            {{ item_properties | remove: discount_property }}
            {%- if qty_offer -%}
              <script type="application/json" data-variant>
                {
                  "currentOptions": {{ item.variant.options | json }},
                  "variants": {{ item.product.variants | json }}
                }
              </script>
            {%- endif -%}
          </div>
        {%- endfor -%}
              
        {%- comment -%}
          #pragma UPSELL MODULE
        {%- endcomment -%}
        
        {%- if settings.MD_upsell_enable -%}
          <upsell-maxdesign></upsell-maxdesign>
        {%- endif -%}
      </div>

      {%- comment -%} 
        #pragma SUMMARY CONTAINER 
      {%- endcomment -%}
    
      <div class="summary__wrapper">
        <div class="summary__container">
          <div class="summary__total--details {%- if automatic_discount or qty_discount_active %} summary__total--details-discount{%- endif -%}{%- if delivery_enable %} summary__total--details-delivery{%- endif -%}">
            <p class="summary__total summary__total--subtotal">
              <span>{{ TRANSLATIONS.subtotal_text }}</span>
              <span class="summary__price summary__price--subtotal" data-subtotal-price="{{ cart.original_total_price }}">{{ cart.original_total_price | money }}</span>
            </p>
            <p class="summary__total summary__total--discount">
              <span class="summary__total--discount-tag">
                <svg viewBox="0 0 18 18"><path d="M17.8 3.1c-.3-.7-1-1.1-1.8-1.1h-5.2c-.5 0-1 .2-1.4.6L2.6 9.5c-.3.2-.4.5-.5.9L.7 8.1c-.6-1-.2-2.2.7-2.7L9.8.5c.5-.3 1-.3 1.5-.2l5 1.3c.8.2 1.3.8 1.5 1.5z" fill-opacity=".55"/><path d="M10.8 2H16c1.1 0 2 .9 2 2v5.2c0 .5-.2 1-.6 1.4l-6.8 6.8c-.8.8-2 .8-2.8 0l-5.2-5.1c-.8-.8-.8-2 0-2.8l6.8-6.9c.4-.4.9-.6 1.4-.6zm2.7 6c.8 0 1.5-.7 1.5-1.5S14.3 5 13.5 5 12 5.7 12 6.5 12.7 8 13.5 8z"/></svg>
                <span class="summary__total--discount-title">
                  {%- if automatic_discount -%}
                    {%- for discount_application in cart.discount_applications -%}{{ discount_application.title | truncate: 20 }}{%- endfor -%}
                  {%- else -%}
                    {{ qty_discount_code | truncate: 20 }}
                  {%- endif -%}
                </span>
                {%- if automatic_discount == nil -%}
                  <svg class="summary__total--discount-close" viewBox="0 0 13 13"><path d="M1.5 1.5l10.05 10.05M11.5 1.5L1.45 11.55" stroke-width="2" fill="none" fill-rule="evenodd" stroke-linecap="round"></path></svg>
                {%- endif -%}
              </span>
              <span class="summary__price summary__price--discount">- {{ cart_discount | money }}</span>
            </p>
            {%- if delivery_enable -%}
              <p class="summary__total summary__total--delivery">
                <span>{{ TRANSLATIONS.delivery_text }}</span>
                <span class="summary__price summary__price--delivery">
                  {{ delivery_price_text }}
                </span>
              </p>
            {%- endif -%}
          </div>
          <p class="summary__total summary__total--total">
            <span>{{ TRANSLATIONS.total_text }}</span>
            <span class="summary__price--total">{{ cart_total | plus: delivery_price_value | money }}</span>
          </p>
          
          <hr class="summary__divider">
          <div class="cart__notes">          
          {% comment %}Scalapay widget. To disable set enabled to false{% endcomment %}
		  {% render 'scalapay-widget', price: cart.total_price, enabled: true %}
          </div>

          
          {%- if TRANSLATIONS.total_subtext != blank -%}
            <p class="summary__info">{{ TRANSLATIONS.total_subtext }}</p>
          {%- endif -%}
          <hr class="summary__divider">
          {%- for block in section.blocks -%}
            {%- if block.settings.block_pos == 'bottom' -%}
              {%- if block.type == 'custom' -%}
                <div class="custom__text" style="--custom-text-color: {{ block.settings.custom_content_color }};">
                  {{ block.settings.custom_content }}
                  <hr class="summary__divider">
                </div>
              {%- elsif block.type == 'timer' -%}
                <div id="cdTimerCart">
                  <p>{{ TIMER.cart_timer_innerText | replace: 'XX', '<span class="cdTimerCart--color"><span id="cart_minutes">&nbsp;&nbsp;</span>:<span id="cart_seconds">&nbsp;&nbsp;</span></span>' }}</p>
                  <hr class="summary__divider">
                </div>
              {%- endif -%}	
            {%- endif -%}
          {%- endfor -%}
          {%- if show_cart_attribute -%}
            <div class="cart__attribute">
              {%- assign attribute_name = ATTRIBUTE.label | handleize -%}
              <label>{{ ATTRIBUTE.label }}</label>
              <select required class="required" name="attributes[{{ attribute_name }}]">
                {%- assign attr_options = ATTRIBUTE.options | newline_to_br | split: '<br />' -%}
                {%- for option in attr_options -%}
                  <option value="{{ option }}" {% if cart.attributes.attribute_name == option %}selected{% endif %}>{{ option }}</option>
                {%- endfor -%}
              </select>
              <hr class="summary__divider">          
            </div>
          {%- endif -%}
          <div class="cartDiscount {% if automatic_discount or qty_discount_active or SAVINGS.promofield == false %}cartDiscount__hidden{% endif %} {% unless SAVINGS.custom_discount %}cartDiscount__btn--hidden{% endunless %}">
            <span>
              <svg viewBox="0 0 18 18"><path d="M17.8 3.1c-.3-.7-1-1.1-1.8-1.1h-5.2c-.5 0-1 .2-1.4.6L2.6 9.5c-.3.2-.4.5-.5.9L.7 8.1c-.6-1-.2-2.2.7-2.7L9.8.5c.5-.3 1-.3 1.5-.2l5 1.3c.8.2 1.3.8 1.5 1.5z" fill-opacity=".55"/><path d="M10.8 2H16c1.1 0 2 .9 2 2v5.2c0 .5-.2 1-.6 1.4l-6.8 6.8c-.8.8-2 .8-2.8 0l-5.2-5.1c-.8-.8-.8-2 0-2.8l6.8-6.9c.4-.4.9-.6 1.4-.6zm2.7 6c.8 0 1.5-.7 1.5-1.5S14.3 5 13.5 5 12 5.7 12 6.5 12.7 8 13.5 8z"/></svg>
              <input type="hidden" value="{{ qty_discount_code }}" name="discount">
              <input placeholder="{{ TRANSLATIONS.promo_code }}" autocomplete="off" type="text" value="" oninput="this.value = this.value.toUpperCase()"/>
              <button name="button" type="button" disabled>
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16"><path d="M16 8.1l-8.1 8.1-1.1-1.1L13 8.9H0V7.3h13L6.8 1.1 7.9 0 16 8.1z"></path></svg>
                <svg class="discount__svg--loader" viewBox="0 0 20 20"><path d="M20 10c0 5.523-4.477 10-10 10S0 15.523 0 10 4.477 0 10 0v2c-4.418 0-8 3.582-8 8s3.582 8 8 8 8-3.582 8-8h2z"></path></svg>
              </button>
            </span>
            <p class="cartDiscount__text">{{ TRANSLATIONS.promofield_text }}</p>
            <hr class="summary__divider">
          </div>
          {%- if section.settings.show_notes -%}
            <div class="cart__notes">
              <label for="CartInstructions">{{ TRANSLATIONS.cart_notes }}</label>
              <textarea name="note" id="CartInstructions" placeholder="{{ TRANSLATIONS.cart_notes_text }}" class="cart__note">{{ cart.note }}</textarea>
            </div>
          {%- endif -%}
          {%- if SAVINGS.enable -%}
            {%- assign total_saving = total_saving | plus: cart.total_discount -%}
            {%- if total_saving > 0 -%}
              <div class="summary__savings">
                <p>{{ TRANSLATIONS.you_save }} <span class="summary__savings--price">{{ total_saving | round | money }}</span></p>
              </div>
            {%- endif -%}
          {%- endif -%}
          {%- if section.settings.cart_terms -%}
            <label class="cart__terms">
              <input type="checkbox" name="terms" required>
              {{ TRANSLATIONS.cart_terms_text | replace: '<p', '<span' | replace: '</p>', '</span>' }}
            </label>
          {%- endif -%}
          <div class="btnCart__sticky">
            <button type="submit" disabled hidden style="display:none"></button>
            <button type="submit" name="checkout" class="btnCart" {% if SAVINGS.discount_quantity_manual_enable %}disabled{% endif %}>
              <span>{{ TRANSLATIONS.button_text }}</span>
              <svg class="btn__svg--loader" viewBox="0 0 20 20"><path d="M20 10c0 5.523-4.477 10-10 10S0 15.523 0 10 4.477 0 10 0v2c-4.418 0-8 3.582-8 8s3.582 8 8 8 8-3.582 8-8h2z"></path></svg>
            </button>
            <div class="cart__sentinel--bottom"></div>
          </div>
          {%- if section.settings.paypal_mode != 'show_checkout' and additional_checkout_buttons -%}
            <div class="additional-checkout-buttons" style="{% if section.settings.paypal_mode == 'show_cart' %}display:block!important;height:auto{% elsif section.settings.paypal_mode == 'hide' %}visibility:hidden!important;height:0;overflow:hidden;padding:0{% endif %}">
              {{ content_for_additional_checkout_buttons }}
            </div>
          {%- endif -%}
          {%- if BADGES.enable -%}
            <div class="cart__badges">
              <span class="cart__badges--lock" {% if BADGES.text == blank %}style="visibility:hidden;height:0;margin-top:-10px"{% endif %}>
                <svg viewBox="0 0 1792 1792"><path d="M640 768h512v-192q0-106-75-181t-181-75-181 75-75 181v192zm832 96v576q0 40-28 68t-68 28h-960q-40 0-68-28t-28-68v-576q0-40 28-68t68-28h32v-192q0-184 132-316t316-132 316 132 132 316v192h32q40 0 68 28t28 68z"/></svg>          
                <span>{{ BADGES.text }}</span>
              </span>
